package com.javaprograms;

public class RemovingDublicateCharatersInaString {

	public static void main(String[] args) {
		
		String name="shwetha";
		String result="";
		
		for(int i=0;i<name.length();i++)
		{
			char ch = name.charAt(i);
			result=result+ch;
			
		}
		System.out.println(result);
		
		}

	}


