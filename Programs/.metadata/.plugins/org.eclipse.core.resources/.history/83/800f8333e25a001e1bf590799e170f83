		  ArrayList al = new ArrayList();
		  al.add(10);
		  al.add(20);
		  al.add(30);
		  al.add(10);
		  for(int i=0;i<al.size()-1;i++)
		  {
			  for(int j=i+1;i<al.size()-1;j++)
			  {
			  if(al.get(i).equals(al.get(j)))
			  {
				  al.remove(i);
			  }
